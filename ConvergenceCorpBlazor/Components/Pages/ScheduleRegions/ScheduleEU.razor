@code{ // 2025/6/8 is sunday
    DateTime chivalrytime = new DateTime(2025,6,9,13,00,00,DateTimeKind.Utc);
    DateTime cutetimeWed =     new DateTime(2025,6,11,18,00,00,DateTimeKind.Utc);
    DateTime cutetimeThur = new DateTime(2025,6,12,18,00,00,DateTimeKind.Utc);
    DateTime skeintime =    new DateTime(2025,6,14,18,30,00,DateTimeKind.Utc);
    DateTime odntime = new DateTime(2025,6,15,14,00,00,DateTimeKind.Utc);
    DateTime LIMon = new DateTime(2025,6,9,16,30,00,DateTimeKind.Utc);
    DateTime LITue = new DateTime(2025,6,10,16,30,00,DateTimeKind.Utc);
    DateTime LIWed = new DateTime(2025,6,11,20,30,00,DateTimeKind.Utc);
    DateTime LIThu = new DateTime(2025,6,12,16,30,00,DateTimeKind.Utc);
    DateTime LIFri = new DateTime(2025,6,13,19,30,00,DateTimeKind.Utc);
    
}
<script>
    var times = document.getElementsByClassName("EUtime");
    for(var time of times){
        //get the date
        let d = new Date(time.innerText);
        //convert the time
        time.innerText = d.toLocaleString([], {
            hour:'numeric',
            minute:'2-digit'
        });

        //get the day
        var listname = '#EU'+ d.toLocaleString('en-US',{weekday:'long'});
        //move to the correct day
        document.querySelector(listname).append(time.parentElement);
    }
    var anytimes = document.getElementsByClassName("EUAnytime");
    for(var any of anytimes){
        //convert the time
        any.innerText = new Date(any.innerText).toLocaleString([], {
            hour:'numeric',
            minute:'2-digit'
        });
    }


</script>
