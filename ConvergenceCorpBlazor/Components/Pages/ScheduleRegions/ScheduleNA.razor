<script>
    var times = document.getElementsByClassName("NAtime");
    for(var time of times){
        let d = new Date(time.innerText);
        time.parentElement.classList.add(d.toLocaleString([],{
            weekday:'long'
        }));
        time.innerText = d.toLocaleString([], {
            hour:'numeric',
            minute:'2-digit'
        });
    }
    var anytimes = document.getElementsByClassName("Anytime");
    for(var any of anytimes){
        any.innerText = new Date(any.innerText).toLocaleString([], {
            hour:'numeric',
            minute:'2-digit'
        });
    }

    var mondays = document.querySelectorAll('.Monday');
    var monlist = document.querySelector('#NAMon');
    for (var mon of mondays){
        monlist.append(mon);
    }
    var tuesdays = document.querySelectorAll('.Tuesday');
    var tuelist = document.querySelector('#NATue');
    for (var tue of tuesdays){
        tuelist.append(tue);
    }
    var wednesdays = document.querySelectorAll('.Wednesday');
    var wedlist = document.querySelector('#NAWed');
    for (var wed of wednesdays){
        wedlist.append(wed);
    }
    var thursdays = document.querySelectorAll('.Thursday');
    var thurlist = document.querySelector('#NAThur');
    for (var thur of thursdays){
        thurlist.append(thur);
    }
    var fridays = document.querySelectorAll('.Friday');
    var frilist = document.querySelector('#NAFri');
    for (var fri of fridays){
        frilist.append(fri);
    }
    var saturdays = document.querySelectorAll('.Saturday');
    var satlist = document.querySelector('#NASat');
    for(var sats of saturdays){
        satlist.append(sats);
    }
    var sundays = document.querySelectorAll('.Sunday');
    var sunlist = document.querySelector('#NASun');
    for (var sun of sundays){
        sunlist.append(sun);
    }
</script>
